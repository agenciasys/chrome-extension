{
    "name": "Agência Sys 2",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Aplicativo do Agências Sys 2.",
    "persistent": false,
    "background": {
        "scripts": ["script.js"]
    },
    "browser_action": {
        "default_icon": "img/favicon-16.png",
        "default_title": "Agência Sys 2",
        "default_popup": "tasks.html"
    },
    "permissions": [
        "https://app2.agenciasys.com/",
        "notifications",
        "storage"
    ],
    "web_accessible_resources": [
        "img/favicon-32.png"
    ],
    "storage": {
        "managed_schema": "schema.json"
    },
    "icons": {
        "16": "img/favicon-16.png",
        "32": "img/favicon-32.png",
        "64": "img/favicon-64.png",
        "96": "img/favicon-96.png"
    }
}